      wallet: "monero-wallet-rpc"
      attributes:
        is_miner: "true"
        hashrate: "30"
        can_receive_distributions: true

    # Miner 4 - 10% hashrate
    - daemon: "monerod"
      wallet: "monero-wallet-rpc"
      attributes:
        is_miner: "true"
        hashrate: "10"
        can_receive_distributions: true

    # Miner 5 - 10% hashrate
    - daemon: "monerod"
      wallet: "monero-wallet-rpc"
      attributes:
        is_miner: "true"
        hashrate: "10"
        can_receive_distributions: true

