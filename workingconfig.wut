# Configuration for 47 agents: 5 miners, 2 seed nodes, 40 regular users
# Simulation duration: 5 hours

general:
  stop_time: "18000s"
  fresh_blockchain: true
  log_level: info

network:
  path: "caida_connected_sparse_with_loops_fixed.gml"
  peer_mode: "Hybrid"
  topology: "Ring"
  seed_nodes:
    - "192.168.1.10:28080"
    - "192.168.1.11:28080"

agents:
  user_agents:
    # Miner 1 - 25% hashrate
    - daemon: "monerod"
      wallet: "monero-wallet-rpc"
      attributes:
        is_miner: "true"
        hashrate: "25"
        can_receive_distributions: true

    # Miner 2 - 25% hashrate
    - daemon: "monerod"
      wallet: "monero-wallet-rpc"
      attributes:
        is_miner: "true"
        hashrate: "25"
        can_receive_distributions: true

    # Miner 3 - 30% hashrate
